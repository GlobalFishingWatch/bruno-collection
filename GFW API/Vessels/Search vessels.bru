meta {
  name: Search vessels
  type: http
  seq: 8
}

get {
  url: {{ host }}/v1/vessels/search
  body: json
  auth: none
}

params:query {
  datasets: global-vessels:v20190502,chile-vessels:v20200331,indonesia-vessels:v20200320
  query: shipname='antonio'+AND+flag+IN+('CHL')
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{token}}
}
