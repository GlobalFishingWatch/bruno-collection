meta {
  name: Search Vessels
  type: http
  seq: 3
}

get {
  url: {{host}}/{{version}}/vessels/search
  body: none
  auth: none
}

params:query {
  datasets: public-global-fishing-vessels:latest
  limit: 10
  offset: 0
  ~query: shipname LIKE '%ANTONIO%'
  ~where: flag='ESP'
  ~includes: POTENTIAL_RELATED_SELF_REPORTED_INFO
  ~registries-info-data: NONE
  ~match-fields: SEVERAL_FIELDS
}


